<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.hello.mapper.ThemeMapper">

    <select id="findAllThemes" resultType="com.example.hello.entity.Theme">
        SELECT * FROM Theme
    </select>
    <select id="getTheme" resultType="com.example.hello.entity.Theme">
        SELECT * FROM Theme WHERE theme_id= #{themeId}
    </select>


</mapper>
