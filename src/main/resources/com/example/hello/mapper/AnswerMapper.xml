<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.hello.mapper.AnswerMapper">
    <select id="registerTheme" resultType="com.example.hello.entity.Answer">
        INSERT INTO public.answer(answer,vote_num,time,theme_id) VALUES (#{anser},0,CURRENT_TIMESTAMP,#{themeId});
    </select>

</mapper>
